{
    "name": "raulfraile/ladybug",
    "description": "Simple and Extensible PHP Dumper",
    "version": "1.0.0-alpha1",
    "keywords": ["ladybug", "dumper", "dump", "var_dump", "print_r"],
    "homepage": "https://github.com/raulfraile/Ladybug",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raul Fraile",
            "email": "raulfraile@gmail.com",
            "homepage": "http://www.raulfb.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": ">=2.0.4",
        "symfony/finder": ">=2.0.4",
        "symfony/dependency-injection": ">=2.0.4",
        "symfony/config": ">=2.0.4",
        "twig/twig": ">=v1.7.0",
        "phpdocumentor/reflection-docblock": "2.0.0a3",
        "jms/serializer": ">=0.11.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "0.1.*@dev",
        "mockery/mockery": "0.8.0",
        "symfony/dom-crawler": ">=2.0.4"
    },
    "suggest": {
        "symfony/yaml": "Required to serialize data to YAML format"
    },
    "autoload": {
        "psr-0": { "Ladybug": "lib/" },
        "files": ["lib/Ladybug/helpers.php"]
    }
}
