<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="metadata_php" class="Ladybug\Metadata\PhpObjectsMetadata">
            <tag name="ladybug.metadata" />
        </service>

        <service id="metadata_aura" class="Ladybug\Metadata\AuraMetadata">
            <tag name="ladybug.metadata" />
        </service>

        <service id="metadata_silex" class="Ladybug\Metadata\SilexMetadata">
            <tag name="ladybug.metadata" />
        </service>

        <service id="metadata_symfony" class="Ladybug\Metadata\SymfonyMetadata">
            <tag name="ladybug.metadata" />
        </service>

        <service id="metadata_twig" class="Ladybug\Metadata\TwigMetadata">
            <tag name="ladybug.metadata" />
        </service>

    </services>
</container>
