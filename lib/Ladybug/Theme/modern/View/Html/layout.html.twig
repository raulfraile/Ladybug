<style>
    {% for file in css %}
    {{ minify_css(file)|raw }}
    {% endfor %}
</style>

<script>
    {% for file in js %}
    {{ include_file(file)|raw }}
    {% endfor %}
</script>

<div class="ladybug" style="width:700px; margin:10px">
    {% for node in nodes %}
        <span class="dump-block">
            {{ render_type(node)|raw }}
        </span>
    {% endfor %}

    <div class="clearfix">
        <span class="label label-important pull-right">{{ callFile }} : {{ callLine }}</span>
    </div>

</div>
